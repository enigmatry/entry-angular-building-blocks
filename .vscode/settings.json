{
    "cSpell.words": [
        "Formly",
        "stylelint"
    ],
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "stylelint.enable": true,
    "stylelint.validate": [
        "css",
        "scss"
    ],
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "files.autoSaveDelay": 500
}