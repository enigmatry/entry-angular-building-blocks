{
    "name": "Entry Angular Building Blocks",
    "entryPointStrategy": "packages",
    "entryPoints": [
        "libs/*"
    ],
    "plugin": ["typedoc-plugin-markdown"],
    "out": "apps/demo-app/src/assets/api",
    "exclude": [
        "libs/stylelint-config",
        "libs/scss-foundation",
        "libs/eslint-config",
        "libs/scss-entry"
    ],
    "customCss": "apps/demo-app/src/styles/modules/typedoc/_typedoc-custom.scss",
    "logLevel": "Info",
    "hideGenerator": true,
    "propertiesFormat": "table",
    "outputFileStrategy": "modules"
}