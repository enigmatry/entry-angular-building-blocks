{
    "name": "Entry Angular Building Blocks",
    "entryPointStrategy": "packages",
    "entryPoints": [
        "projects/*"
    ],
    "out": "generated-documentation",
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "exclude": [
        "projects/eslint-config/*",
        "projects/scss-foundation/*",
        "projects/stylelint-config/*",
    ],
    "cleanOutputDir": true,
    "excludeNotDocumented": true,
    "readme": "README.md",
    "logLevel": "Verbose"
}