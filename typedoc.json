{
    "name": "Entry Angular Building Blocks",
    "entryPointStrategy": "packages",
    "entryPoints": [
        "libs/*"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "out": "apps/demo-app/src/assets/api",
    "exclude": [
        "libs/stylelint-config",
        "libs/scss-foundation",
        "libs/eslint-config",
        "libs/scss-entry"
    ],
    "logLevel": "Info",
    "outputFileStrategy": "modules",
    "propertiesFormat": "table",
    "hideGenerator": true,
    "hidePageHeader": true,
    "hideBreadcrumbs": true,
    "hideInPageTOC": true
}