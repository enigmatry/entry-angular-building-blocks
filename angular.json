{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@enigmatry/entry-table": {
      "projectType": "library",
      "root": "projects/entry-table",
      "sourceRoot": "projects/entry-table/src",
      "prefix": "entry",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/entry-table/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/entry-table/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/entry-table/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/entry-table/src/test.ts",
            "tsConfig": "projects/entry-table/tsconfig.spec.json",
            "karmaConfig": "projects/entry-table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/entry-table/**/*.ts",
              "projects/entry-table/**/*.html"
            ]
          }
        }
      }
    },
    "@enigmatry/entry-form": {
      "projectType": "library",
      "root": "projects/entry-form",
      "sourceRoot": "projects/entry-form/src",
      "prefix": "entry",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/entry-form/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/entry-form/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/entry-form/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/entry-form/src/test.ts",
            "tsConfig": "projects/entry-form/tsconfig.spec.json",
            "karmaConfig": "projects/entry-form/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/entry-form/**/*.ts",
              "projects/entry-form/**/*.html"
            ]
          }
        }
      }
    },
    "@enigmatry/entry-components": {
      "projectType": "library",
      "root": "projects/entry-components",
      "sourceRoot": "projects/entry-components",
      "prefix": "entry",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/entry-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/entry-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/entry-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/entry-components/test.ts",
            "tsConfig": "projects/entry-components/tsconfig.spec.json",
            "karmaConfig": "projects/entry-components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/entry-components/**/*.ts",
              "projects/entry-components/**/*.html"
            ]
          }
        }
      }
    },
    "@enigmatry/entry-components-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "docs/entry-components-docs",
      "sourceRoot": "docs/entry-components-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/enigmatry/entry-components-docs",
            "index": "docs/entry-components-docs/src/index.html",
            "main": "docs/entry-components-docs/src/main.ts",
            "polyfills": "docs/entry-components-docs/src/polyfills.ts",
            "tsConfig": "docs/entry-components-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "docs/entry-components-docs/src/favicon.ico",
              "docs/entry-components-docs/src/assets"
            ],
            "styles": [
              "docs/entry-components-docs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "docs/entry-components-docs/src/environments/environment.ts",
                  "with": "docs/entry-components-docs/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "@enigmatry/entry-components-docs:build:production"
            },
            "development": {
              "browserTarget": "@enigmatry/entry-components-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "@enigmatry/entry-components-docs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "docs/entry-components-docs/src/test.ts",
            "polyfills": "docs/entry-components-docs/src/polyfills.ts",
            "tsConfig": "docs/entry-components-docs/tsconfig.spec.json",
            "karmaConfig": "docs/entry-components-docs/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "docs/entry-components-docs/src/favicon.ico",
              "docs/entry-components-docs/src/assets"
            ],
            "styles": [
              "docs/entry-components-docs/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "docs/entry-components-docs/**/*.ts",
              "docs/entry-components-docs/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
